<script>
  var i = [['A1', 'A2', 'E', 'E'], 
           ['B1', 'B2', 'E', 'E'], 
          ['E','E','E','E'],
          ['E','E','E','E']];
   function myFunction (num) {
     pieceList = [];
     for (var q = 0; q < 4; q++) {
       for (var m = 0; m < 4; m++) {
         if (i[q][m] === num) {
           l = q++;
           pieceList.push(i[l][m]);
           pieceList.push(i[q][m]);
           
           i[q][m] = pieceList[0];
           i[l][m] = pieceList[1];
           
         }
     } 
     } 
   for(var e = 0; e < i.length; e++) {
     console.log(i[e]);
   }
   }
</script>
  
  
<!-- Single button -->
<div class="btn-group">
  <a onclick='myFunction(i[0][0])' class="btn btn-default">
    A1 <span class="caret"></span>
  </a>
    <a onclick='myFunction(i[0][1])' class="btn btn-default">
    A2 <span class="caret"></span>
  </a>
    <a onclick='myFunction(i[0][2])' class="btn btn-default">
    A3 <span class="caret"></span>
  </a>
    <a onclick='myFunction(i[0][3])' class="btn btn-default">
    A4 <span class="caret"></span>
  </a>
  <br>
    <a onclick='myFunction(i[1][0])' class="btn btn-default">
    B1 <span class="caret"></span>
  </a>
    <a onclick='myFunction(i[1][1])' class="btn btn-default">
    B2 <span class="caret"></span>
