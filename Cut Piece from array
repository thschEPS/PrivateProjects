for (var i = 0; i < pieceSetUp.length; i++) {
                      if (pieceSetUp[i].position === rightPieceList[0]){
                        var index = _.indexOf(pieceSetUp,pieceSetUp[i]);
                        if (index > -1) {
                          pieceSetUp.splice(index, 1);
                          console.log(pieceSetUp);
                        }
                      }
                    }
