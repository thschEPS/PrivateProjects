<body>
  <script>
    var i = [[
      'Q', 'E', 'E'
    ],[
      'E', 'E', 'E'
    ],[
      'E', 'E', 'E'
    ]];
    
    var pieceList = [], 
        count = 0; 
   
    function my(piece) {
        if (count === 0) {
            for (var q = 0; q < i.length; q++) {
                for (var m = 0; m < i.length; m++) {
                    if (i[q][m] === piece) {
                        pieceList.push(i[q][m]);
                      console.log(pieceList);
                      count++;
                    }
                    

                    }
                }
            
        } else if (count > 0){
            for (var r = 0; r < i.length; r++) {
                for (var k = 0; k < i.length; k++) {
                    if (i[r][k] === piece) {
                        pieceList.push(i[r][k]);
                    }
                    i[r][k] = pieceList[0];
                    i[q][m] = pieceList[1];

                }
            }
        }
console.log(i);
    }
    
  </script>
<button onclick='my(i[0][0])'>Piece</button>
  <button onclick='my(i[0][1])'>Piece</button>
  <button onclick='my(i[0][2])'>Piece</button>
<br>
  <button onclick='my(i[1][0])'>Piece</button>
  <button onclick='my(i[1][1])'>Piece</button>
  <button onclick='my(i[1][2])'>Piece</button>
<br>
  <button onclick='my(i[2][0])'>Piece</button>
  <button onclick='my(i[2][1])'>Piece</button>
  <button onclick='my(i[2][2])'>Piece</button>

  
